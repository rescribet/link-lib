{
  "name": "link-lib",
  "version": "1.0.0-rc4-datafactory",
  "description": "The Link library for creating information based applications",
  "main": "dist/link-lib.umd.js",
  "module": "dist/link-lib.es6.js",
  "sideEffects": false,
  "typings": "dist/typings/link-lib.d.ts",
  "files": [
    "dist/link-lib.es6.js",
    "dist/link-lib.umd.js",
    "dist/typings/**/*.*",
    "package.json",
    "tsconfig.json",
    "yarn.lock"
  ],
  "repository": "https://github.com/fletcher91/link-lib.git",
  "scripts": {
    "build": "rollup -c && tsc -p tsconfig-types.json",
    "doc": "rm -rf dist/docs/* && typedoc --tsconfig tsconfig-types.json --exclude **/*.spec.ts --excludeExternals --excludePrivate --mode file --theme minimal --target ES6 --includeDeclarations --out dist/docs/ src/",
    "doc:deploy": "gh-pages -d ./dist/docs",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "prebuild": "rm -rf dist/*",
    "prepublish": "npm run build && npm run doc",
    "postpublish": "npm run doc:deploy",
    "pretest": "npm run lint",
    "test": "jest --coverage"
  },
  "author": "Fletcher91 <thom@argu.co>",
  "license": "LGPL-3.0",
  "dependencies": {
    "murmurhash-js": "^1.0.0"
  },
  "peerDependencies": {
    "@ontologies/as": ">=1.0.1",
    "@ontologies/core": ">=1.5.0",
    "@ontologies/schema": ">=1.0.0",
    "@ontologies/shacl": ">=1.0.0",
    "@ontologies/xsd": ">=1.0.0",
    "http-status-codes": ">= 1.x",
    "rdflib": ">= 0.19.x"
  },
  "devDependencies": {
    "@ontola/memoized-hash-factory": "^1.0.1",
    "@ontologies/as": "^1.0.1",
    "@ontologies/core": "=1.5.2-strict-17",
    "@ontologies/dcterms": "^1.0.0",
    "@ontologies/owl": "^1.0.0",
    "@ontologies/rdf": "^1.0.0",
    "@ontologies/rdfs": "^1.0.0",
    "@ontologies/schema": "^1.0.0",
    "@ontologies/shacl": "^1.0.0",
    "@ontologies/xsd": "^1.0.0",
    "@types/firefox-webext-browser": "^67.0.2",
    "@types/jest": "^24.0.19",
    "@types/murmurhash-js": "^1.0.3",
    "@types/node": "^12.11.2",
    "core-js": "^3.3.3",
    "esdoc": "^1.1.0",
    "gh-pages": "^2.1.1",
    "http-status-codes": ">= 1.x",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "n-quads-parser": "=2.0.3-ontologies",
    "rdflib": "^1.0.4",
    "rollup": "^1.25.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.24.3",
    "solid-auth-cli": "^1.0.8",
    "solid-auth-client": "^2.3.1",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.7.0-beta"
  },
  "jest": {
    "automock": false,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/utilities/DisjointSet.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "preset": "ts-jest",
    "setupFiles": [
      "core-js",
      "./jest-plugins"
    ],
    "testMatch": [
      "**/*.spec.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testURL": "http://example.org/resources/5",
    "transform": {
      "\\.ts$": "ts-jest"
    }
  }
}
